# Telegram Group Manager - Environment Variables
# Copy this file to .env and fill in the values

# ============ Telegram ============
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_API_ID=your_api_id
TELEGRAM_API_HASH=your_api_hash
TELEGRAM_WEBHOOK_URL=https://your-project.vercel.app/api/telegram/webhook
TELEGRAM_WEBHOOK_SECRET=your_webhook_secret_string

# ============ Supabase ============
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# ============ Upstash Redis ============
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token

# ============ Cloudinary (Optional) ============
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# ============ Security ============
JWT_SECRET=your_jwt_secret_key_at_least_32_characters
ADMIN_INVITE_CODE=your_admin_invite_code

# ============ Application ============
NODE_ENV=production
PROJECT_NAME=telegram-group-manager
